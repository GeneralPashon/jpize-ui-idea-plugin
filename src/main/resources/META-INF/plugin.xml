<idea-plugin>
    <id>online.generalpashon.jpize-ui-idea-plugin</id>
    <name>JPUI Syntax Highlighting</name>
    <version>23.12.1</version>
    <idea-version since-build="183.0"/>

    <vendor email="goloborodkojava@gmail.com" url="https://generalpashon.online">GeneralPashon</vendor>

    <description><![CDATA[
    Jpize-Engine UI markup language syntax highlight plugin.
    ]]></description>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <fileType
                name="JPUI File"
                implementationClass="online.generalpashon.jpizeuihighlighter.lang.JpuiFileType"
                fieldName="INSTANCE"
                language="JPUI"
                extensions=".jpui"/>

        <lang.syntaxHighlighterFactory
                language="JPUI"
                implementationClass="online.generalpashon.jpizeuihighlighter.highlighter.JpuiHighlighterFactory"/>

        <lang.parserDefinition
                language="JPUI"
                implementationClass="online.generalpashon.jpizeuihighlighter.parser.JpuiParserDefinition"/>

        <completion.contributor
                language="JPUI"
                implementationClass="online.generalpashon.jpizeuihighlighter.codecompletion.JpuiCompletionContributor"/>
    </extensions>
</idea-plugin>